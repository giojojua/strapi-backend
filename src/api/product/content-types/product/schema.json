{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "model": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "decimal"
    },
    "year": {
      "type": "integer",
      "required": true
    },
    "transmission": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "product_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product-category.product-category"
    },
    "engine": {
      "type": "string",
      "required": true
    },
    "position": {
      "type": "integer",
      "required": true
    }
  }
}
